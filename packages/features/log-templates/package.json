{
  "name": "@packages/log-templates",
  "version": "0.0.0",
  "description": "IPN log templates for AIMS",
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/index.css",
    "./index.d.ts": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "wait": "wait-on dist/index.js dist/index.css",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "chroma-js": "2.4.2",
    "flat": "^6.0.1",
    "seedrandom": "^3.0.5",
    "zustand": "^4.4.1"
  },
  "peerDependencies": {
    "@tanstack/react-table": "^8.9.7",
    "chart.js": "^4.4.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0"
  },
  "devDependencies": {
    "@packages/tsconfig": "workspace:*",
    "@packages/tsup-config": "workspace:*",
    "@types/chroma-js": "^2.4.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "daisyui": "^3.3.1",
    "eslint": "^8.15.0",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.20",
    "tailwind-config": "workspace:*",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "wait-on": "^7.0.1"
  }
}
